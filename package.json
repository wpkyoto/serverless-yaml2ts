{
  "name": "serverless-yaml2ts",
  "description": "Simple convertor from serverless.yml to serverless.ts",
  "version": "0.0.2",
  "author": "Hidetaka Okamoto",
  "bin": {
    "sls-yaml2ts": "./bin/run"
  },
  "bugs": "https://github.com/wpkyoto/serverless-yaml2ts/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "js-yaml": "^3.14.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^10",
    "@types/serverless": "^1.72.5",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/wpkyoto/serverless-yaml2ts",
  "keywords": [
    "oclif"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "sls-yaml2ts"
  },
  "repository": "wpkyoto/serverless-yaml2ts",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
